# triangular()
import random 
import matplotlib.pyplot as plt 
nums = [] 
mu = 100 
sigma = 50 
for i in range(10000): 
  temp = random.triangular(mu, sigma) 
  nums.append(temp) 
plt.hist(nums, bins = 200) 
plt.show() 

# betavariate()
import random 
import matplotlib.pyplot as plt 
nums = [] 
mu = 100 
sigma = 50 
for i in range(10000): 
  temp = random.betavariate(mu, sigma) 
  nums.append(temp) 
plt.hist(nums, bins = 200) 
plt.show() 

